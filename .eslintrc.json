{
	"env": {
		"browser": true,
		"mocha": true,
		"es6": true
	},
	"parserOptions": {
		"sourceType": "module",
		"ecmaVersion": 2020,
		"ecmaFeatures": {
			"jsx": true,
			"impliedStrict": true
		}
	},
	"globals": {
		"sap": true,
		"jQuery": true,
		"$": true,
		"require": true
	},
	"rules": {
		"no-multiple-empty-lines": [1, {"max": 1}],
		"brace-style": [1, "1tbs", {"allowSingleLine":true}],
		"camelcase": 1,
		"curly": 0,
		"eqeqeq": 2,
		"guard-for-in": 2,
		"no-extend-native": 2,
		"wrap-iife": 0,
		"no-use-before-define": 0,
		"new-cap": 0,
		"no-caller": 2,
		"no-empty": 2,
		"no-irregular-whitespace": 2,
		"no-new": 0,
		"no-plusplus": 0,
		"quotes": 0,
		"no-undef": 2,
		"no-unused-vars": [1, {"vars":"all", "args":"after-used"}],
		"strict": 0,
		"max-params": 0,
		"max-depth": 0,
		"max-statements": 0,
		"no-var": 1,
		"semi": 1,
		"no-cond-assign": 0,
		"no-debugger": 0,
		"no-eq-null": 0,
		"no-eval": 0,
		"no-unused-expressions": 0,
		"block-scoped-var": 0,
		"no-iterator": 0,
		"linebreak-style": 0,
		"comma-style": [2, "last"],
		"comma-spacing": [1, {"after": true}],
		"no-loop-func": 0,
		"no-multi-str": 0,
		"require-yield": 0,
		"valid-typeof": 0,
		"no-proto": 0,
		"no-script-url": 0,
		"no-shadow": 2,
		"dot-notation": 2,
		"no-new-func": 0,
		"no-new-wrappers": 0,
		"no-invalid-this": 0,
		"space-infix-ops": 1,
		"key-spacing": [1, {"beforeColon": false, "afterColon": true, "mode":"strict"}],
		"keyword-spacing": [1, {"before": true, "after": true}],
		"space-before-blocks": 1,
		"max-len": [1, 150, 5, {
			"ignoreComments": false,
			"ignoreTrailingComments": false,
			"ignoreUrls": true,
			"ignoreStrings": true,
			"ignoreRegExpLiterals": true,
			"ignoreTemplateLiterals": true
		}],
		"indent": ["error", "tab"],
		"no-mixed-spaces-and-tabs": 1,
		"prefer-const": 1
	}
}
