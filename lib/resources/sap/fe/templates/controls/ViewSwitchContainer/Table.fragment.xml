<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:macro="sap.fe.macros"
	xmlns:core="sap.ui.core"
	xmlns:unittest="http://schemas.sap.com/sapui5/preprocessorextension/sap.fe.unittesting/1"
>
	<macro:Table
		unittest:id="TablePropertyExpressionTest"
		parentEntitySet="{entitySet>}"
		id="{visualizationDefinition>annotation/id}"
		collection="{visualizationDefinition>annotation/collection}"
		presentation="{presentationContext>annotationPath}"
		filters="{visualizationDefinition>control/filters}"
		navigationPath="{visualizationDefinition>annotation/navigationPath}"
		headerVisible="{visualizationDefinition>control/headerVisible}"
		enableExport="{visualizationDefinition>control/enableExport}"
		tableType="{visualizationDefinition>control/type}"
		rowPress="{visualizationDefinition>annotation/row/press}"
		rowAction="{visualizationDefinition>annotation/row/action}"
		rowHighlighting="{visualizationDefinition>annotation/row/rowHighlighting}"
		rowNavigated="{visualizationDefinition>annotation/row/rowNavigated}"
		selectionMode="{visualizationDefinition>annotation/selectionMode}"
		showCreate="{visualizationDefinition>annotation/show/create}"
		showDelete="{visualizationDefinition>annotation/show/delete}"
		creationMode="{visualizationDefinition>annotation/create/mode}"
		disableAddRowButtonForEmptyData="{visualizationDefinition>control/disableAddRowButtonForEmptyData}"
		createAtEnd="{visualizationDefinition>annotation/create/append}"
		enableAutoScroll="{visualizationDefinition>control/enableAutoScroll}"
		autoBindOnInit="{visualizationDefinition>annotation/autoBindOnInit}"
		enableControlVM="{visualizationDefinition>annotation/enableControlVM}"
		p13nMode="{visualizationDefinition>annotation/p13nMode}"
		filterInterface="{= ${converterContext>filterBarId} ? ${converterContext>filterBarId} : undefined}"
		columns="{visualizationDefinition>columns}"
		actions="{visualizationDefinition>actions}"
		threshold="{visualizationDefinition>annotation/threshold}"
		noDataText="{= ${visualizationDefinition>annotation/isEntitySet} ? undefined : ${sap.fe.i18n>T_OP_TABLE_NO_DATA_TEXT} }"
		editMode='{visualizationDefinition>annotation/show/update}'
		createMode='{= ${visualizationDefinition>annotation/isEntitySet} ? "false" : "{localUI>/createMode}"}'
		busy='{= ${visualizationDefinition>annotation/isEntitySet} ? "{ui>/busy}" : "{ui>/busyLocal/"+${visualizationDefinition>annotation/id}+"}"}'
		onCallAction=".editFlow.onCallAction"
		onDataFieldForIBN=".handlers.onDataFieldForIntentBasedNavigation"
		onCreate=".editFlow.createDocument"
		create="{visualizationDefinition>annotation/create}"
		onPatchSent=".editFlow.handlePatchSent"
		onPatchCompleted=".editFlow.handlePatchCompleted"
		onDelete=".editFlow.deleteMultipleDocuments"
		selectedContextsModel="localUI"
		requestGroupId="$auto.Workers"
		updateGroupId="$auto"
		onDataReceived=".editFlow.handleErrorOfTable"
		onContextChange=".editFlow.onTableContextChange"
		onChange=".handlers.onFieldValueChange"
		editModePropertyBinding="{= ${visualizationDefinition>annotation/isEntitySet} ? 'Display' : 'ui>/editMode' }"
		variantSelected=".handlers.onVariantSelected"
		variantSaved=".handlers.onVariantSaved"
		onPaste="{= ${visualizationDefinition>annotation/isEntitySet} ? null : '._onPasteInTable' }"
		onPasteButtonPressed="{= ${visualizationDefinition>annotation/isEntitySet} ? null : '._onPasteTableButtonPressed' }"
		tableDelegate="{= ${converterContext>isAlp} ? '{name: \'sap/fe/templates/AnalyticalListPage/table/FETableDelegate\', payload: {collectionName: \'' + ${visualizationDefinition>annotation/entityName} + '\' }}' : undefined }"
	>
	</macro:Table>
</core:FragmentDefinition>
