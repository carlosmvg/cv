        xmlns="sap.m"
        xmlns:core="sap.ui.core">
    <ViewSettingsDialog
            confirm="handleSortDialogConfirm">
        <sortItems>
            <ViewSettingsItem text="{i18n>Message.SortDialogId}" key="id" selected="true"/>
