{
	"_version": "1.12.0",
	"sap.app": {
		"id": "myCV",
		"type": "application",
		"i18n": "i18n/i18n.properties"
	},
	"sap.ui5": {
		"dependencies": {
			"minUI5Version": "1.75.0",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {},
				"sap.ui.unified": {}
			}
		},
		"rootView": {
			"viewName": "myCV.view.App",
			"type": "XML"
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "myCV.i18n.i18n",
					"supportedLocales": ["en", "es"],
					"fallbackLocale": "en"
				}
			}
		},
		"resources": {
			"css": [
				{
					"uri": "css/styles.css"
				}
			]
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"viewPath": "myCV.view",
				"controlAggregation": "pages",
				"controlId": "app",
				"transition": "slide",
				"clearControlAggregation": false
			},
			"routes": [
				{
					"name": "Home",
					"pattern": "",
					"greedy": true,
					"target": "Home"
				}
			],
			"targets": {
				"Home": {
					"viewName": "Home",
					"viewLevel": 1
				}
			}
		}
	}
}
